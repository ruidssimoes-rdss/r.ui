{
  "name": "hyena-studio-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @hyena-studio/docs dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "lint:design": "pnpm --filter @hyena-studio/lint build && pnpm --filter @hyena-studio/lint test",
    "clean": "pnpm -r clean",
    "storybook": "pnpm --filter @hyena-studio/storybook start",
    "chromatic": "pnpm --filter @hyena-studio/storybook chromatic"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
