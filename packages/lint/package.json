{
  "name": "@r-ui/lint",
  "version": "0.1.0",
  "description": "Design and accessibility linter for r.ui components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "r-ui-lint": "./bin/r-ui-lint.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "test": "node dist/cli.js ../react-native/src/components/"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.4",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "bin"
  ],
  "license": "MIT"
}
