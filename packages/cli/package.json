{
  "name": "hyena-studio",
  "version": "0.1.0",
  "description": "CLI for Hyena Studio - pull design tokens into your project",
  "keywords": [
    "design-system",
    "tokens",
    "cli",
    "hyena",
    "figma",
    "tailwind"
  ],
  "license": "MIT",
  "author": "Hyena Studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyena-studio/hyena"
  },
  "bin": {
    "hyena-studio": "./bin/hyena-studio.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "prompts": "^2.4.2",
    "picocolors": "^1.0.0",
    "lz-string": "^1.5.0"
  },
  "devDependencies": {
    "@types/lz-string": "^1.3.34",
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
